# BiyeToCode 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 分析LLM配置（用于论文分析）
ANALYZER_LLM_API_KEY="sk-..."
ANALYZER_LLM_MODEL="模型ID"
ANALYZER_LLM_BASE_URL=https://example/api/v1

# 代码LLM配置（用于代码生成和修复）

CODER_LLM_API_KEY="sk-..."
CODER_LLM_MODEL="模型ID"
CODER_LLM_BASE_URL=https://example/api/v1

# 代码执行配置
MAX_CODE_RETRIES=5
MAX_CHAT_TURNS=30
CODE_TIMEOUT=300

# 路径配置
UPLOAD_DIR=uploads
OUTPUT_DIR=output

# 日志配置
LOG_LEVEL=INFO
